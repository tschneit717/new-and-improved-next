{
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "editor.insertSpaces": true,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.autoFixOnSave": true,
  "source.fixAll.eslint": true,
  "eslint.alwaysShowStatus": true,
  "eslint.options": {
    "extensions": [".js", ".jsx", ".ts", ".tsx"]
  },
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "eslint.run": "onType",
  "eslint.lintTask.enable": true,
  "eslint.packageManager": "npm",
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "eslint.format.enable": true,
  "standardFormat.formatOnSave": true,
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#e49253",
    "activityBar.background": "#e49253",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#c8f6dc",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#e49253",
    "statusBar.background": "#dd7627",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#b55e1c",
    "statusBarItem.remoteBackground": "#dd7627",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#dd7627",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#dd762799",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.color": "#dd7627",
}